
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url     = env("DATABASE_URL")
}



model User {
  id      Int    @id @default(autoincrement())
  name    String
  email   String @unique
  phone   String
  adress  String
  password String
  role      Role     @default(USER)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}